
@{
    Layout = null;
}

@using MyHowest;

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Student</title>
</head>
<body>

    <h2>Studenten</h2>

    <ul>
        @foreach (Student student in ViewBag.Studenten)
        {
            <li>@student.Id</li>
            <li>@student.Naam</li>
            <li>@student.AfstudeerGraad</li>
            <li>@student</li>
            <hr />
        }
    </ul>


</body>
</html>
